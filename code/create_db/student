USE cs252test;

CREATE TABLE student (
		sname varchar(20) NOT NULL,
		name varchar(20) NOT NULL,
		password varchar(20) NOT NULL,
		rollno varchar(10) NOT NULL,
		dname varchar(20) NOT NULL,
		PRIMARY KEY(sname),
		FOREIGN KEY (dname) REFERENCES department(dname)
);

